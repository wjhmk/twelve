
<%= image_tag @post.image.url(:medium) %>

<h1>title:<%= @post.title %></h1>
<h1>link:<%= @post.link %></h1>
<h1>description:<%= @post.description %></h1>
<h1>id:<%= @post.id %></h1>
<p>=========================</p>
<h1>name of editer:<%= @post.user.name %></h1>
